    "test:watch": "tsc -p . --watch",
    "test": "node -r ./jasmine/globalSetup.cjs ./node_modules/jasmine/bin/jasmine.js --config=jasmine/jasmine.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/enbock/energy-flow-component.git"
  },
  "keywords": [
    "ai-assistant",
    "clean",
    "code",
    "visualisation"
  ],
  "author": "Endre Bock <dev@itbock.de>",
  "license": "GPL-3.0",
  "homepage": "https://www.itbock.de",
  "devDependencies": {
    "@enbock/ts-jsx": "*",
    "@testing-library/dom": "*",
    "@testing-library/jasmine-dom": "*",
    "@testing-library/user-event": "*",
    "@types/jasmine": "*",
    "@types/jsdom": "*",
    "@types/node": "*",
    "@types/webpack": "*",
    "@types/webpack-env": "*",
    "copy-webpack-plugin": "*",
    "css-loader": "*",
    "html-webpack-plugin": "*",
    "jasmine": "*",
    "jsdom": "*",
    "ts-loader": "*",
    "ts-node": "*",
    "tsconfig-paths": "*",
    "typescript": "*",
    "webpack": "*",
    "webpack-cli": "*",
    "webpack-dev-server": "*"
  }
