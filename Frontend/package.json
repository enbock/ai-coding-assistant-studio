  "version": "0.0.0-dev1",
  "description": "A studio to code with an AI assistant.",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --open --mode development --no-open --no-hot --stats-error-details",
    "build": "webpack --mode production",
